body{background-image:url(/Style/Mobile/images/bg_bill.png)}
.process{margin:10px auto;background-color:#CAE0EA;padding:6px 30px 20px 30px;width:50%;text-align:center;box-shadow:1px 1px 1px 0px gray;}
.process h1{height:50px;
			line-height:60px;color:#3B6C9D;
			text-shadow:1px 1px 1px gray;font-size:20px;border-bottom:2px solid #3B6C9D;}
.process li{height:32px;font-color:#9B9C9C;
 margin-top:20px;font-size:20px;line-height:32px;background-repeat:no-repeat;background-size:32px 32px;background-position:10px 0;padding-left:40px;}
.process li:nth-child(1){
	background-image:url(/Style/H/images/bill/index0.png);}
.process li:nth-child(2){
	background-image:url(/Style/H/images/bill/index1.png);}
.process li:nth-child(3){
	background-image:url(/Style/H/images/bill/index2.png);}
.process li:nth-child(4){
	background-image:url(/Style/H/images/bill/index3.png);}
.process li:nth-child(5){
	background-image:url(/Style/H/images/bill/index4.png);}
.formDiv{
	position:fixed;  top: 0%;  left: 0%;  width: 100%;height:100%;background-color: black;  z-index:1000;  display:none;
}
#form1{width:80%;margin:30px auto;background-color:#fff;padding:10px;}
#form1 input{width:100%;height:40px;margin-top:20px;}
#form1 input[type=text]{
border:none;border-bottom:1px solid #666;border-radius:0;text-indent:20px;
}
.input1{
	background-color:#2AA198;color:#fff;
	font-size:20px;
	border-radius:10px;
}
.input2{
	background-color:#666;color:#fff;
	font-size:20px;
	border-radius:10px;
}
#showForm{margin-top:28px;text-decoration:underline;color:#3B6C9D;}
#form1 p{margin-top:10px;text-align:center;}