/* sliderkit */
.sliderkit{display:none;position:relative;overflow:hidden;text-align:left;width:100%!important;height:358px;overflow:hidden;}
.sliderkit a,.sliderkit a:hover{text-decoration:none;}
.sliderkit a:focus{outline:1px dotted #000;/*optional*/}
.sliderkit img{border:0;width:100%;}

/*---------------------------------
 *  Navigation
 *---------------------------------*/
.sliderkit .sliderkit-nav{z-index:10;position:absolute;text-align:center;}
/* Nav > Clip */
.sliderkit .sliderkit-nav-clip{position:absolute;margin:0 auto;}
.sliderkit .sliderkit-nav-clip ul{position:relative;left:0;top:570px;z-index:1000;list-style:none;margin:0;padding:0;}
.sliderkit .sliderkit-nav-clip ul li{float:left;}
.sliderkit .sliderkit-nav-clip ul li a{display:block;overflow:hidden;}

/*---------------------------------
 *  Buttons
 *---------------------------------*/
.sliderkit .sliderkit-btn{z-index:10;}
/*---------------------------------
 *  Panels
 *---------------------------------*/
.sliderkit .sliderkit-panel{z-index:1;position:absolute;overflow:hidden;}
.sliderkit .sliderkit-panel-active{z-index:5;}
.sliderkit .sliderkit-panel-old{z-index:4;}
/* Panels > Overlay */
.sliderkit .sliderkit-panel .sliderkit-panel-textbox{position:absolute;z-index:1;}
.sliderkit .sliderkit-panel .sliderkit-panel-text{position:absolute;z-index:3;top:0;left:0;}
.sliderkit .sliderkit-panel .sliderkit-panel-overlay{position:absolute;z-index:2;top:0;left:0;}
/*---------------------------------
 *  Loader
 *---------------------------------*/
.contentslider-std .sliderkit-panels{width:100%;height:100%;overflow:hidden;background:#fff url(ajax-loader.gif) no-repeat center center;}
.contentslider-std .sliderkit-panel h2{margin:5px 0 20px;}

/* Panels buttons */
.contentslider-std .sliderkit-go-btn{color:#FFFFFF;cursor:pointer;font-size:50px;font-weight:normal;height:58px;}
.contentslider-std .sliderkit-go-btn{position:absolute;text-align:center;top:50%;width:58px;line-height:58px;margin-top:-29px;}
.contentslider-std .sliderkit-go-prev{left:-58px;background:url(../images/l-arr.png) no-repeat center center;}
.contentslider-std .sliderkit-go-next{right:-58px;background:url(../images/r-arr.png) no-repeat center center;}
.contentslider-std .sliderkit-go-prev a{display:block;width:19px;height:28px;background:#333;}
.contentslider-std .sliderkit-go-prev a{margin:16px auto 0 auto;font-size:0;line-height:0;text-indent:-4000px;}
.contentslider-std .sliderkit-go-next a{display:block;width:18px;height:27px;background:url(../images/r-arr.png)}
.contentslider-std .sliderkit-go-next a{margin:16px auto 0 auto;font-size:0;line-height:0;text-indent:-4000px;}
.contentslider-std .sliderkit-go-btn span{display:none;}
